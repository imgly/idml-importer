{
  "name": "@imgly/idml-importer",
  "version": "1.1.13",
  "module": "dist/browser.js",
  "types": "dist/browser.d.ts",
  "type": "module",
  "description": "Import IDML files into the Creative Editor Ecosystem",
  "keywords": [
    "creative-editor-sdk",
    "indesign",
    "import",
    "idml"
  ],
  "author": {
    "name": "IMG.LY GmbH",
    "email": "support@img.ly",
    "url": "https://img.ly"
  },
  "bugs": {
    "email": "support@img.ly"
  },
  "homepage": "https://img.ly/showcases/cesdk/indesign-template-import/web",
  "devDependencies": {
    "@cesdk/engine": "^1.40.0",
    "@cesdk/node": "^1.40.0",
    "@changesets/cli": "^2.29.4",
    "bun": "^1.2.21",
    "bun-plugin-dts": "^0.2.0",
    "bun-types": "latest",
    "glob": "7.0.3",
    "jest-image-snapshot": "^6.2.0",
    "jsdom": "^22.1.0"
  },
  "peerDependencies": {
    "@cesdk/engine": "^1.40.0 ",
    "@cesdk/node": "^1.40.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@cesdk/engine": {
      "optional": true
    },
    "@cesdk/node": {
      "optional": true
    }
  },
  "files": [
    "LICENSE.md",
    "README.md",
    "dist/",
    "dist/assets/"
  ],
  "scripts": {
    "download-example-files": "gsutil -m cp -r \"gs://idml-importer-customer-samples/\" ./src/test/examples",
    "prepare-example-files": "./scripts/prepare-files.sh test/examples",
    "prepare-files": "./scripts/prepare-files.sh",
    "build": "rm -rf dist/ && mkdir -p dist/assets && cp -r ./assets/ dist/ && bun run build.ts",
    "test": "bun test src/test/idml-parser.test.ts",
    "publish": "npm run build && npm publish --access public",
    "deploy-assets": "bash -c 'source .env && aws s3 sync --endpoint-url $S3_ENDPOINT_URL dist s3://$S3_BUCKET_NAME/@imgly/idml-importer/$npm_package_version'"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "exports": {
    ".": {
      "node": {
        "import": {
          "types": "./dist/node.d.ts",
          "default": "./dist/node.js"
        },
        "require": {
          "types": "./dist/node.d.ts",
          "default": "./dist/node.js"
        }
      },
      "default": {
        "import": {
          "types": "./dist/browser.d.ts",
          "default": "./dist/browser.js"
        },
        "require": {
          "types": "./dist/browser.d.ts",
          "default": "./dist/browser.js"
        }
      }
    }
  }
}
