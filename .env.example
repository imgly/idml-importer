# R2 CDN Deployment Configuration
# Uses 1Password secret references - requires `op` CLI

# Cloudflare R2 credentials (uses 1Password references)
RCLONE_CONFIG_R2_TYPE=s3
RCLONE_CONFIG_R2_PROVIDER=Cloudflare
RCLONE_CONFIG_R2_ACCESS_KEY_ID=op://Shared/Cloudflare IMGLY-CDN R2 API Key/Access Key ID
RCLONE_CONFIG_R2_SECRET_ACCESS_KEY=op://Shared/Cloudflare IMGLY-CDN R2 API Key/Secret Access Key
RCLONE_CONFIG_R2_ENDPOINT=op://Shared/Cloudflare IMGLY-CDN R2 API Key/endpoint
